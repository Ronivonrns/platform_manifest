<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android-review.googlesource.com/" />

  <remote name="github"
           fetch="https://github.com/" />
		   
  <remote  name="los"
           fetch="https://github.com/LineageOS"
           revision="lineage-15.0" />

  <default revision="refs/tags/android-8.0.0_r17"
           remote="aosp"
           sync-j="4" />

  <!-- AOSP packages -->
  <include name="snippets/aosp.xml" />

  <!-- Custom packages -->
  <include name="snippets/custom_packages.xml" />

  <!-- Devices HALs -->
  <include name="snippets/hals.xml" />

  <!-- Linux packages -->
  <include name="snippets/linux.xml" />

</manifest>
